@model Subestaciones.Models.Clases.RedCD

@{
    ViewBag.Title = "Servicio de corriente directa";
}

@*@section pagetitle{
        <div class="page-title">
            <h1>Insertar banco de capacitor</h1>
        </div>
    }*@


<!-- END PAGE HEAD-->
<!-- BEGIN PAGE CONTENT BODY -->
<div class="page-content">
    <div class="container-fluid">
        <!-- BEGIN PAGE BREADCRUMBS -->
        <ul class="page-breadcrumb breadcrumb">
            <li>
                <a href="@Url.Action("Index","Sub_RedCorrienteDirecta")">Lista de servicios de corriente directa</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span>Detalles</span>
            </li>
        </ul>
        <!-- END PAGE BREADCRUMBS -->
        <!-- BEGIN PAGE CONTENT INNER -->
        <div class="page-content-inner">
            <div class="row">
                <div class="col-md-12">

                    <div class="portlet box blue">
                        <div class="portlet-title">
                            <div class="caption">
                                <i class="ti-stats-down"></i>Detalles del servicio de corriente directa "@Html.DisplayFor(model => model.NombreServicioCD)"
                            </div>
                        </div>
                        <div class="portlet-body form">

                            @using (Html.BeginForm())
                            {
                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                @Html.AntiForgeryToken()

                                <!-- BEGIN FORM-->

                                <div class="horizontal-form">
                                    <div class="form-body">
                                      

                                        <div class="row">

                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.NombreSub, htmlAttributes: new { @class = "control-label" })
                                                    @Html.EditorFor(model => model.NombreSub, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })

                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.NombreServicioCD, htmlAttributes: new { @class = "control-label" })
                                                    @Html.EditorFor(model => model.NombreServicioCD, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.idVoltaje, htmlAttributes: new { @class = "control-label" })
                                                    @Html.EditorFor(model => model.idVoltaje, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })

                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.UsoRed, htmlAttributes: new { @class = "control-label" })
                                                    @Html.EditorFor(model => model.UsoRed, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })

                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.ControlAislamBarras, htmlAttributes: new { @class = "control-label" })
                                                    @Html.EditorFor(model => model.ControlAislamBarras, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    @Html.LabelFor(model => model.Observaciones, htmlAttributes: new { @class = "control-label " })
                                                    @Html.TextAreaFor(model => model.Observaciones, new { @class = "form-control", @readonly="readonly" })
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-actions right">
                                            <a class="btn btn-primary" href="@Url.Action("Index","Sub_RedCorrienteDirecta")"><i class="fa fa-list"></i> Listado de servicios</a>
                                            <a class="btn btn-warning" href="@Url.Action("Edit","Sub_RedCorrienteDirecta", new {Id_ServicioCD = Model.ID  })"><i class="fa fa-list"></i>Editar</a>
                                        </div>
                                    </div>
                                </div>

                            }
                            <!-- END FORM-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END PAGE CONTENT INNER -->
    </div>
</div>
<!-- END PAGE CONTENT BODY -->
<!-- END CONTENT BODY -->
