                                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                    @Html.AntiForgeryToken()

                                    <div class="horizontal-form">
                                        <div class="form-body">

                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        @Html.LabelFor(model => model.NombreSub, htmlAttributes: new { @class = "control-label" })
                                                        @Html.EditorFor(model => model.NombreSub, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                                                       
                                                    </div>
                                                </div>
