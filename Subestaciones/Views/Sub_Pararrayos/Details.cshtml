        <div class="container-fluid">
            <!-- BEGIN PAGE BREADCRUMBS -->
            <ul class="page-breadcrumb breadcrumb">
                <li>
                    <a href="@Url.Action("Index","Sub_Pararrayos")">Lista de pararrayos</a>
                    <i class="fa fa-circle"></i>
                </li>
                <li>
                    <span>Detalles</span>
