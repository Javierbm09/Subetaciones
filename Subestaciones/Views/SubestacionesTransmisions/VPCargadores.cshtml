@model List<Subestaciones.Models.Clases.BancoCargadores>

<table id="datatable_Cargadores" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>Tipo cargador</th>
            <th>Corriente</th>
            <th>Tensión</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>
                    @item.tipoCargador

                </td>

                <td>
                    @item.corriente
                </td>

                <td>
                    @item.volCD
                </td>

            </tr>
        }

    </tbody>
</table>
