@model IEnumerable<Subestaciones.Models.Clases.TransformadorTransmision>

<div class="portlet light">
    <div class="portlet-title">
        <div class="caption font-dark">
        </div>
        <div class="tools"> </div>
    </div>
    <div class="portlet-body" id="TablaResumen">
        <table id="table_Transf" class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Nro Empresa</th>
                    <th>Nro Inventario</th>
                    <th>Capacidad</th>
                    <th>Tensión Primaria (kV)</th>
                    <th>Tensión Secundaria (kV)</th>
                    <th>Tensión Terciaria (kV)</th>
                    <th>Fabricante</th>
                    <th>Año Fabricación</th>
                    <th>Corriente Primaria </th>
                    <th>Corriente Secundaria </th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {

                    <tr>
                        <td>
                            @item.Nombre
                        </td>

                        <td>
                            @item.Numemp
                        </td>

                        <td>
                            @item.NumeroInventario
                        </td>
                        <td>
                            @item.Capacidad
                        </td>
                        <td>
                            @item.Id_VoltajePrim
                        </td>

                        <td>
                            @item.Id_Voltaje_Secun
                        </td>
                        <td>
                            @item.VoltajeTerciario
                        </td>
                        <td>
                            @item.idFabricante
                        </td>

                        <td>
                            @item.AnnoFabricacion
                        </td>
                        <td>
                            @item.CorrientePrimaria
                        </td>
                        <td>
                            @item.CorrienteSecundaria
                        </td>

                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>