@model IEnumerable<Subestaciones.Models.Clases.Inspecciones>
<div class="portlet light">
    <div class="portlet-title">
        <div class="caption font-dark">
        </div>
        <div class="tools"> </div>
    </div>
    <div class="portlet-body" id="TablaResumen">
        <table id="table_Inspec" class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Tipo de inspección</th>
                    <th>Fecha</th>
                    <th>Realizado Por</th>



                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>

                        <td>
                            @item.TipoInspeccion

                        </td>
                        <td>
                            @item.Fecha

                        </td>
                        <td>
                            @item.RealizadoPor

                        </td>


                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>