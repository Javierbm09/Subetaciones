@model IEnumerable<Subestaciones.Models.Clases.RedCD>

<table id="datatable_RedCDT" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>Nombre del Servicio</th>
            <th>Tensión</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @item.NombreServicioCD
                    <input type="hidden" id="idServ" value="@item.Id_ServicioCD" />
                </td>
                <td>
                    @item.idVoltaje
                </td>

            </tr>
        }

    </tbody>
</table>
