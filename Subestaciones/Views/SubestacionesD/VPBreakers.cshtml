@model List<Subestaciones.Models.Sub_DesconectivoSubestacion>
<div class="portlet light">
    <div class="portlet-title">
        <div class="caption font-dark">
        </div>
        <div class="tools"> </div>
    </div>
    <div class="portlet-body" id="TablaResumen">
        <table id="datatable_breakers" class="table table-striped table-bordered table-hover">
            <thead>
                @*<tr>
                        <th colspan="2" style="align-content:center">Transformadores</th>
                    </tr>*@
                <tr>
                    <th data-field="id" data-switchable="false">Código</th>
                    <th data-field="name">Tensión Nominal</th>
                    <th data-field="name">Corriente Nominal</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.CodigoDesconectivo</td>
                        <td>@item.TensionNominal</td>
                        <td>@item.CorrienteNominal</td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
