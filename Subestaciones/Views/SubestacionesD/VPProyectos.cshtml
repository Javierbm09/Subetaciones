@model List<Subestaciones.Models.Clases.Proyectos>
<div class="portlet light">
    <div class="portlet-title">
        <div class="caption font-dark">
        </div>
        <div class="tools"> </div>
    </div>
    <div class="portlet-body" id="TablaResumen">
        <table id="table_proyectD" class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Nombre del Proyecto</th>
                    <th>Estado</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {

                    <tr>
                        <td>
                            @item.NombreProyecto
                        </td>

                        <td>
                            @item.EstadoDelProyecto
                        </td>


                    </tr>
                }

            </tbody>
        </table>
</div>
</div>