@model IEnumerable<Subestaciones.Models.Sub_RedCorrienteDirecta>
<div class="portlet light">
    <div class="portlet-title">
        <div class="caption font-dark">
        </div>
        <div class="tools"> </div>
    </div>
    <div class="portlet-body" id="TablaResumen">
        <table id="datatable_RedCD" class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Nombre del Servicio</th>
                    <th>Tensión</th>


                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>

                        <td>
                            @item.NombreServicioCD
                            <input type="hidden" id="idServ" value="@item.Id_ServicioCD" />
                        </td>
                        <td>
                            @item.idVoltaje
                        </td>

                    </tr>
                }

            </tbody>
        </table>
</div>
</div>