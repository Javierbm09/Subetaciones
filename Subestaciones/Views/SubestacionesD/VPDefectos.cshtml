@model List<Subestaciones.Models.Clases.DefectoViewModel>
<div class="portlet light">
    <div class="portlet-title">
        <div class="caption font-dark">
        </div>
        <div class="tools"> </div>
    </div>
    <div class="portlet-body" id="TablaResumen">
        <table id="table_defectosD" class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>Estado</th>
                    <th>Fecha</th>
                    <th>Defecto</th>
                    <th>Elemento</th>
                    <th>Material</th>
                    <th>Dimensión</th>
                    <th>Observaciones</th>


                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {

                    <tr>
                        <td>
                            @item.Estado
                        </td>

                        <td>
                            @item.Fecha
                        </td>

                        <td>
                            @item.Defecto
                        </td>
                        <td>
                            @item.Elemento
                        </td>
                        <td>
                            @item.Material
                        </td>

                        <td>
                            @item.Dimension
                        </td>
                        <td>
                            @item.Observaciones
                        </td>


                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>