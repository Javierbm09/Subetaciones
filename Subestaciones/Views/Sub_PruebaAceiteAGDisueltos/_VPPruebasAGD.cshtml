@model List<Subestaciones.Models.Clases.AnalisisGD>

<table id="datatable" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>Subestaci&oacute;n</th>
            <th>Transformador</th>
            <th>No Serie</th>
            <th>No emp</th>
            <th>Realizado en</th>
            <th>Ejecutador por</th>
            <th>Fecha</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            string llaves = item.Id_Transf + "|" + item.Id_EAdminTransf + "|" + item.Fecha;

            <tr>
                <td>
                    @item.subestacion
                </td>
                <td>
                    @item.Nombre
                </td>
                <td>
                    @item.NoSerie
                </td>
                <td>
                    @item.Numemp
                </td>
                <td>
                    @item.RealizadoenLaboraorio
                </td>
                <td>
                    @item.EjecutadoPor
                </td>
                <td>
                    @item.Fecha
                </td>
                <td style="width: 125px;">
                    <a href="@Url.Action("Details","Sub_PruebaAceiteAGDisueltos", new { t = item.Id_Transf , EA = item.Id_EAdminTransf, fecha = item.Fecha  } )" class="btn btn-info mt-sweetalert btn-xs" title="Detalles"><i class="fa fa-eye"></i>  </a>
                    <a href="@Url.Action("Edit","Sub_PruebaAceiteAGDisueltos", new { t = item.Id_Transf , EA = item.Id_EAdminTransf, fecha = item.Fecha } )" class="btn btn-warning mt-sweetalert btn-xs" title="Editar"><i class="fa fa-pencil"></i>  </a>
                    <a href="javascript:;" id="@llaves" title="Eliminar" class="btn btn-default mt-sweetalert btn-xs" data-title="¿Est&aacute; seguro de eliminar la prueba seleccionada?" data-message="¡Los valores que est&aacute;n en la fila a eliminar no se podr&aacute;n recuperar!"
                       data-type="info" data-show-confirm-button="true" data-confirm-button-class="btn-success" data-show-cancel-button="true" data-cancel-button-class="btn-default" data-close-on-confirm="false" data-close-on-cancel="false"
                       data-confirm-button-text='¡S&iacute;, eliminar!' data-cancel-button-text='¡No, cancelar!' data-popup-title-success="¡Eliminado!" data-popup-message-success="El mantenimiento ha sido eliminado." data-popup-title-cancel="Cancelado"
                       data-popup-message-cancel="La acci&oacute;n de eliminar ha sido cancelada."><i class="fa fa-trash-o"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>