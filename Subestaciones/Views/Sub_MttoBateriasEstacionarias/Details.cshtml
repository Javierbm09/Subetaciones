                                                            @Html.EditorFor(model => model.DatosCBBVM.TipoBateria, new { htmlAttributes = new { @class = "form-control", @disabled = true } })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <div class="form-group" id="cargarCapacidad">
                                                            @Html.LabelFor(model => model.DatosCBBVM.TensionBateria, htmlAttributes: new { @class = "control-label" })
                                                            @Html.EditorFor(model => model.DatosCBBVM.TensionBateria, new { htmlAttributes = new { @class = "form-control", @disabled = true } })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <div class="form-group" id="cargarClase">
                                                            @Html.LabelFor(model => model.DatosCBBVM.ClaseBateria, htmlAttributes: new { @class = "control-label" })
                                                            @Html.EditorFor(model => model.DatosCBBVM.ClaseBateria, new { htmlAttributes = new { @class = "form-control", @disabled = true } })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <div class="form-group" id="cargarClase">
                                                            @Html.LabelFor(model => model.DatosCBBVM.CapacidadBateria, htmlAttributes: new { @class = "control-label" })
                                                            @Html.EditorFor(model => model.DatosCBBVM.CapacidadBateria, new { htmlAttributes = new { @class = "form-control", @disabled = true } })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <div class="form-group" id="cargarClase">
                                                            @Html.LabelFor(model => model.DatosCBBVM.NombreServicioCD, htmlAttributes: new { @class = "control-label" })
                                                            @Html.EditorFor(model => model.DatosCBBVM.NombreServicioCD, new { htmlAttributes = new { @class = "form-control", @disabled = true } })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="panel">
                                            <div class="panel-heading">Datos generales del mantenimiento a baterías estacionarías</div>

                                            <div class="panel-body">
                                                <div class="col-md-12">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Sub_MBE.EstadoExtVasos, htmlAttributes: new { @class = "control-label" })
                                                            @Html.EditorFor(model => model.Sub_MBE.EstadoExtVasos, new { htmlAttributes = new { @class = "form-control", @disabled = true } })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Sub_MBE.EstadoAprieteBornes, htmlAttributes: new { @class = "control-label" })
                                                            <div class="radio">
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.EstadoAprieteBornes, 1)
                                                                    Sí
                                                                </label>
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.EstadoAprieteBornes, 0)
                                                                    No
                                                                </label>
                                                            </div>
                                                            @Html.ValidationMessageFor(model => model.Sub_MBE.EstadoAprieteBornes, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Sub_MBE.LimpiezaBornes, htmlAttributes: new { @class = "control-label" })
                                                            <div class="radio">
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.LimpiezaBornes, 1)
                                                                    Sí
                                                                </label>
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.LimpiezaBornes, 0)
                                                                    No
                                                                </label>
                                                            </div>
                                                            @Html.ValidationMessageFor(model => model.Sub_MBE.LimpiezaBornes, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Sub_MBE.AprieteBornes, htmlAttributes: new { @class = "control-label" })
                                                            <div class="radio">
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.AprieteBornes, 1)
                                                                    Sí
                                                                </label>
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.AprieteBornes, 0)
                                                                    No
                                                                </label>
                                                            </div>
                                                            @Html.ValidationMessageFor(model => model.Sub_MBE.AprieteBornes, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
