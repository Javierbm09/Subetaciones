                                            <div class="panel-heading">Datos generales del mantenimiento</div>
                                            <div class="panel-body">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        @Html.LabelFor(model => model.Sub_MBE.subestacion, htmlAttributes: new { @class = "control-label" })

                                                        @Html.DropDownListFor(model => model.Sub_MBE.subestacion, ViewBag.ListaSub as SelectList,
"Seleccione un elemento de la lista", htmlAttributes: new { @class = "form-control select2", @id = "codSub", @disabled = true })
                                                        @Html.ValidationMessageFor(model => model.Sub_MBE.subestacion, "", new { @class = "text-danger" })
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        @Html.LabelFor(model => model.Sub_MBE.TipoMtto, htmlAttributes: new { @class = "control-label" })

