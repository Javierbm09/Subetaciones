                                                            @Html.LabelFor(model => model.Sub_MBE.AplicacionVaselina, htmlAttributes: new { @class = "control-label" })
                                                            <div class="radio">
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.AplicacionVaselina, 1)
                                                                    Sí
                                                                </label>
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.AplicacionVaselina, 0)
                                                                    No
                                                                </label>
                                                            </div>
                                                            @Html.ValidationMessageFor(model => model.Sub_MBE.AplicacionVaselina, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Sub_MBE.LimpiezaPanelBaterias, htmlAttributes: new { @class = "control-label" })
                                                            <div class="radio">
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.LimpiezaPanelBaterias, 1)
                                                                    Sí
                                                                </label>
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.LimpiezaPanelBaterias, 0)
                                                                    No
                                                                </label>
                                                            </div>
                                                            @Html.ValidationMessageFor(model => model.Sub_MBE.LimpiezaPanelBaterias, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Sub_MBE.LimpiezaPanelCargador, htmlAttributes: new { @class = "control-label" })
                                                            <div class="radio">
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.LimpiezaPanelCargador, 1)
                                                                    Sí
                                                                </label>
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.LimpiezaPanelCargador, 0)
                                                                    No
                                                                </label>
                                                            </div>
                                                            @Html.ValidationMessageFor(model => model.Sub_MBE.LimpiezaPanelCargador, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                    <div class="col-md-2">
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Sub_MBE.AterramientoPaneles, htmlAttributes: new { @class = "control-label" })
                                                            <div class="radio">
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.AterramientoPaneles, 1)
                                                                    Sí
                                                                </label>
                                                                <label>
                                                                    @Html.RadioButtonFor(model => model.Sub_MBE.AterramientoPaneles, 0)
                                                                    No
                                                                </label>
                                                            </div>
                                                            @Html.ValidationMessageFor(model => model.Sub_MBE.AterramientoPaneles, "", new { @class = "text-danger" })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-3">
                                                        <div class="form-group">
                                                            @Html.LabelFor(model => model.Sub_MBE.NivelElectrolito, htmlAttributes: new { @class = "control-label" })

                                                            @Html.DropDownListFor(model => model.Sub_MBE.NivelElectrolito, ViewBag.nivel as IEnumerable<SelectListItem>,
    "Seleccione un elemento de la lista", htmlAttributes: new { @class = "form-control select2", @disabled = true })
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label class="control-label " for="Observaciones">Observaciones</label>
                                                <textarea class="form-control" cols="20" data-val="true" data-val-length="El campo Observaciones debe ser una cadena con una longitud máxima de 750." data-val-length-max="750" id="Sub_MBE_Observaciones" name="Sub_MBE.Observaciones" rows="5" disabled>@ViewBag.Observaciones</textarea>
                                                <span class="field-validation-valid text-danger" data-valmsg-for="Observaciones" data-valmsg-replace="true"></span>
                                            </div>
                                        </div>

                                        <div class="panel">
                                            <div class="tabbable-line boxless tabbable-reversed ">
                                                <ul class="nav nav-tabs">
                                                    <li>
                                                        <a href="#tabTransf_2" data-toggle="tab" id="tabTransf2">Datos de los vasos </a>
                                                    </li>
                                                    <li>
                                                        <a href="#tabTransf_3" data-toggle="tab" id="tabTransf3">Instrumentos utilizados durante el mantenimiento </a>
                                                    </li>
                                                </ul>
                                                <div class="tab-content">
                                                    <div class="tab-pane active" id="tabTransf_2">
                                                        <div class="panel-body">
                                                            <div class=" col-md-12" id="TablaSub_MBEV">

                                                                <table style="width:50%" class="table">
                                                                    <thead>
                                                                        <tr>
                                                                            <th>Nro. Vasos</th>
                                                                            <th id="densidad">Densidad (mg/cm3)</th>
                                                                            <th id="voltaje">Voltaje (V)</th>
                                                                            <th id="temp">Temperatura (°C)</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        @{
                                                                            if (Model != null && Model.Sub_MBEV != null)
                                                                            {
                                                                                for (int i = 0; i < ViewBag.nroVasos; i++)
                                                                                {
                                                                                    <tr>

                                                                                        <td>
                                                                                            @Html.TextBoxFor(model => model.Sub_MBEV[i].nroVaso, new { @class = "col-md-2 form-control", @id = "Sub_MBEV_nroVaso" + i })
                                                                                        </td>
                                                                                        <td>
                                                                                            @Html.TextBoxFor(model => model.Sub_MBEV[i].densidad, new { @class = "col-md-2 form-control", @id = "Sub_MBEV_densidad" + i })
                                                                                        </td>
                                                                                        <td>
                                                                                            @Html.TextBoxFor(model => model.Sub_MBEV[i].voltaje, new { @class = "col-md-2 form-control", @id = "Sub_MBEV_voltaje" + i })
                                                                                        </td>
                                                                                        <td>
                                                                                            @Html.TextBoxFor(model => model.Sub_MBEV[i].temperatura, new { @class = "col-md-2 form-control", @id = "Sub_MBEV_temperatura" + i })
                                                                                        </td>

                                                                                    </tr>
                                                                                }
                                                                            }
                                                                        }
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="tab-pane" id="tabTransf_3">
                                                        <div class="panel-body">
                                                            <div id="TablaInstrumentosBateriasEstacionarias"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-actions right">
                                            <a class="btn btn-primary" href="@Url.Action("Index","Sub_MttoBateriasEstacionarias")"><i class="fa fa-list"></i> Listado de mantenimientos</a>
                                            <a class="btn btn-warning" href="@Url.Action("Edit","Sub_MttoBateriasEstacionarias", new {id_MBE = Model.DatosCBBVM.id_MttoBatEstacionarias, id_Bateria = Model.DatosCBBVM.Id_Bateria, redEA = Model.DatosCBBVM.EA_RedCD, cod = Model.Sub_MBE.subestacion } )" class="btn btn-warning mt-sweetalert btn-xs" title="Editar"><i class="fa fa-pencil"></i> Editar</a>
                                            @*<a class="btn btn-warning" href="@Url.Action("Edit","Sub_MttoBateriasEstacionarias", new { id_MBE = Model.Sub_MBEI.id_MttoBatEstacionarias, id_Bateria = Model.Sub_MBEI.id_Bateria, redEA = Model.Sub_MBEI.EA_RedCD, cod = Model.Sub_MBE.subestacion })"><i class="fa fa-pencil"></i> Editar</a>*@
                                        </div>

                                    </div>
                                </div>
                            }
                            <!-- END FORM-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END PAGE CONTENT INNER -->
    </div>
</div>
<!-- END PAGE CONTENT BODY -->
<!-- END CONTENT BODY -->

