    <script>
        var idMBE = @ViewBag.IdMttoBE;
        var idB = @ViewBag.IdBateria;
        var ea_red = @ViewBag.RedEA;

        $(document).ready(function () {
            inicializar_select2();

            function inicializar_select2() {
                $(".select2").select2({
                    placeholder: "Seleccione un elemento de la lista"
                });
            }

            fecha = new Date();
            fechaini = new Date(fecha.getFullYear(), fecha.getMonth(), fecha.getDate(), 0, 0);
            fechafin = new Date(fecha.getFullYear(), fecha.getMonth(), fecha.getDate(), 23, 59);

            $("#fechahora").datetimepicker({
                language: 'es',
                todayBtn: 1,
                autoclose: 1,
                todayHighlight: 1,
                dateFormat: 'dd/mm/yyyy',
                linkFormat: 'dd/mm/yyyy',
                showMeridian: 1,
                startDate: fechaini,
                endDate: fechafin,
                forceParse: 1,
                startView: 2,
                maxView: 2
            }).datetimepicker('setDate', fecha);

            $('.js-switch').bootstrapSwitch({
                onText: "Si",
                offText: "No"
            });

            $('.radio :input').prop('disabled', true);


            for (i = 0; i < @ViewBag.NroVasos; i++) {
                llenarTabla();
            }

            function llenarTabla() {

                //var nombreNroVaso = "Sub_MBEV[" + i + "].nroVaso";
                var idNroVaso = "Sub_MBEV_nroVaso" + i;
                var nroVasoText = "#" + idNroVaso;

                //var nombreDensidad = "Sub_MBEV[" + i + "].densidad";
                var idDensidad = "Sub_MBEV_densidad" + i;
                var DensidadText = "#" + idDensidad;

                //var nombreVoltaje = "Sub_MBEV[" + i + "].voltaje";
                var idVoltaje = "Sub_MBEV_voltaje" + i;
                var VoltajeText = "#" + idVoltaje;

                //var nombreTemperatura = "Sub_MBEV[" + i + "].temperatura";
                var idTemperatura = "Sub_MBEV_temperatura" + i;
                var TemperaturaText = "#" + idTemperatura;

                $(nroVasoText).attr("disabled", true);
                $(DensidadText).attr("disabled", true);
                $(VoltajeText).attr("disabled", true);
                $(TemperaturaText).attr("disabled", true);
            }

        });

    </script>

    <script src="~/Scripts/js/jsTablaInstrumentosBateriasEstacionariasDetails.js"></script>
}
