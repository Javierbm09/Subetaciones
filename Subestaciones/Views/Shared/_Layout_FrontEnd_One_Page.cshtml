<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="shortcut icon" href="favicon.ico">
    <base href='@Url.Content("~/")'>
    <!-- Fonts START -->
    <link href="@Url.Content("~/Content/fuentes/css/OpenSans_PathwayGothicOne_PTSansNarrow_SourceSansPro.css")" rel="stylesheet" type="text/css">
    <!-- Fonts END -->
    <!-- Global styles START -->
    <link href="@Url.Content("~/Scripts/assets/global/plugins/font-awesome/css/font-awesome.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Scripts/assets/global/plugins/bootstrap/css/bootstrap.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Scripts/assets/global/plugins/slider-revolution-slider/rs-plugin/css/settings.css")" rel="stylesheet">
    <!-- Global styles END -->
    <!-- Page level plugin styles START -->
    <link href="@Url.Content("~/Scripts/assets/global/plugins/fancybox/source/jquery.fancybox.css")" rel="stylesheet">
    <!-- Page level plugin styles END -->
    <!-- Theme styles START -->
    <link href="@Url.Content("~/Content/assets/global/css/components.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/assets/frontend/onepage/css/style.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/assets/frontend/onepage/css/style-responsive.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/assets/frontend/onepage/css/themes/orange.css")" rel="stylesheet" id="style-color">
    <link href="@Url.Content("~/Content/assets/frontend/onepage/css/custom.css")" rel="stylesheet">
</head>
<body>
    @Html.Partial("_OnePage_Style_Customizer")
    @Html.Partial("_OnePage_Header")

    @RenderBody()

    @Html.Partial("_OnePage_Pre_Footer")
    @Html.Partial("_OnePage_Footer")

    <script src="@Url.Content("~/Scripts/assets/global/plugins/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/jquery-migrate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/assets/global/plugins/slider-revolution-slider/rs-plugin/js/jquery.themepunch.revolution.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/slider-revolution-slider/rs-plugin/js/jquery.themepunch.tools.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/frontend/onepage/scripts/revo-ini.js")" type="text/javascript"></script>

    <!-- Core plugins BEGIN (required only for current page) -->
    <script src="@Url.Content("~/Scripts/assets/global/plugins/fancybox/source/jquery.fancybox.pack.js")" type="text/javascript"></script><!-- pop up -->
    <script src="@Url.Content("~/Scripts/assets/global/plugins/jquery.easing.js")"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/jquery.parallax.js")"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/jquery.scrollTo.min.js")"></script>
    <script src="@Url.Content("~/Scripts/assets/frontend/onepage/scripts/jquery.nav.js")"></script>
    <!-- Core plugins END (required only for current page) -->

    <script src="@Url.Content("~/Scripts/assets/frontend/onepage/scripts/layout.js")" type="text/javascript"></script>
    <script type="text/javascript">
        jQuery(document).ready(function () {
            Layout.init();
        });
    </script>
    <!-- END PAGE LEVEL JAVASCRIPTS -->
    @RenderSection("scripts", required: false)
</body>
</html>
