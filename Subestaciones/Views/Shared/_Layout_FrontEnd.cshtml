<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title | Subestaciones - SIGERE</title>
    <link rel="shortcut icon" href="~/favicon.ico">
    <!-- Fonts START -->
    <link href="@Url.Content("~/Content/fuentes/css/OpenSans_PTSansNarrow_SourceSansPro.css")" rel="stylesheet" type="text/css">
    <!-- Fonts END -->
    <!-- Global styles START -->
    <link href="@Url.Content("~/Scripts/assets/global/plugins/font-awesome/css/font-awesome.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Scripts/assets/global/plugins/bootstrap/css/bootstrap.min.css")" rel="stylesheet">
    <!-- Global styles END -->
    <!-- Page level plugin styles START -->
    <link href="@Url.Content("~/Scripts/assets/global/plugins/fancybox/source/jquery.fancybox.css")" rel="stylesheet">
    <link href="@Url.Content("~/Scripts/assets/global/plugins/carousel-owl-carousel/owl-carousel/owl.carousel.css")" rel="stylesheet">
    <link href="@Url.Content("~/Scripts/assets/global/plugins/slider-revolution-slider/rs-plugin/css/settings.css")" rel="stylesheet">
    <!-- Page level plugin styles END -->
    <!-- Theme styles START -->
    <link href="@Url.Content("~/Content/assets/global/css/components.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/assets/frontend/layout/css/style.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/assets/frontend/pages/css/style-revolution-slider.css")" rel="stylesheet"><!-- metronic revo slider styles -->
    <link href="@Url.Content("~/Content/assets/frontend/layout/css/style-responsive.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/assets/frontend/layout/css/themes/orange.css")" rel="stylesheet" id="style-color">
    <link href="@Url.Content("~/Content/assets/frontend/layout/css/custom.css")" rel="stylesheet">
</head>
<body class="corporate">
    @Html.Partial("_FrontEnd_Style_Customizer")
    @Html.Partial("_FrontEnd_Top_Bar")
    @Html.Partial("_FrontEnd_Header")
    @Html.Partial("_FrontEnd_Slider")
    <div class="main">
        <div class="container">
            @RenderBody()
        </div>
    </div>
    @Html.Partial("_FrontEnd_Pre_Footer")
    @Html.Partial("_FrontEnd_Footer")

    <script src="@Url.Content("~/Scripts/assets/global/plugins/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/jquery-migrate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/frontend/layout/scripts/back-to-top.js")" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL JAVASCRIPTS (REQUIRED ONLY FOR CURRENT PAGE) -->
    <script src="@Url.Content("~/Scripts/assets/global/plugins/fancybox/source/jquery.fancybox.pack.js")" type="text/javascript"></script><!-- pop up -->
    <script src="@Url.Content("~/Scripts/assets/global/plugins/carousel-owl-carousel/owl-carousel/owl.carousel.min.js")" type="text/javascript"></script><!-- slider for products -->
    <!-- BEGIN RevolutionSlider -->

    <script src="@Url.Content("~/Scripts/assets/global/plugins/slider-revolution-slider/rs-plugin/js/jquery.themepunch.plugins.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/slider-revolution-slider/rs-plugin/js/jquery.themepunch.revolution.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/global/plugins/slider-revolution-slider/rs-plugin/js/jquery.themepunch.tools.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/assets/frontend/pages/scripts/revo-slider-init.js")" type="text/javascript"></script>
    <!-- END RevolutionSlider -->
    <script src="@Url.Content("~/Scripts/assets/frontend/layout/scripts/layout.js")" type="text/javascript"></script>
    <script type="text/javascript">
        jQuery(document).ready(function () {
            Layout.init();
            Layout.initOWL();
            RevosliderInit.initRevoSlider();
            //Layout.initTwitter();
            Layout.initFixHeaderWithPreHeader(); /* Switch On Header Fixing (only if you have pre-header) */
            Layout.initNavScrolling();
        });
    </script>
    <!-- END PAGE LEVEL JAVASCRIPTS -->
    @RenderSection("scripts", required: false)
</body>
</html>
